        parent="Widget.AppCompat.Light.ActionBar.Solid">
        <item name="android:background">@null</item>
        <item name="background">@null</item>

    </style>
    <style name="spinnerItemStyle" parent="android:Widget.TextView.SpinnerItem">

    </style>
